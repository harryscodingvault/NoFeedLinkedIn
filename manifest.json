{
  "manifest_version": 2,
  "name": "LinkedIn Simple",
  "description": "Delete access to feed in LinkedIn",
  "version": "1.0.0",
  "icons": {
    "16": "16_icon.png"
  },
  "page_action": {
    "default_icon": {
      "16": "16_icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "css": ["main.css"],
      "js": ["content.js"]
    }
  ]
}
